    "mustache": "4.0.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.2",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^5.3.0",
    "react-table": "^7.1.0",
