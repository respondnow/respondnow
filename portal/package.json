{
  "name": "respondnow-io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config config/webpack.dev.js",
    "build": "rm -rf dist && webpack --config config/webpack.prod.js",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:staged": "sh scripts/eslint-staged.sh",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,scss}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,json,scss}\"",
    "test": "TZ=UTC jest --maxWorkers=4",
    "test:silent": "TZ=UTC jest --maxWorkers=4 --silent",
    "test:clean": "TZ=UTC jest --clearCache",
    "test:watch": "yarn test --watch",
    "test:coverage": "TZ=UTC jest --coverage --colors --maxWorkers=2",
    "services": "node scripts/services.mjs",
    "strings:check": "node scripts/yamlStringsCheck.js",
    "strings:unused": "node scripts/findUnusedStrings.js",
    "strings:sort": "yaml-sort -i src/strings/strings.en.yaml",
    "typecheck": "tsc",
    "typecheck:staged": "sh scripts/typecheck-staged.sh",
    "clean": "rm -rf dist && rm -rf node_modules/.cache",
    "generate-certificate": "sh scripts/generate-certificate.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/respondnow/respond.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/respondnow/respond/issues"
  },
  "homepage": "https://github.com/respondnow/respond#readme",
  "lint-staged": {
    "strings.*.yaml": [
      "yarn strings:unused -r",
      "yarn strings:check"
    ],
    "src/**/*.+(json)": [
      "yarn format"
    ],
    "src/**/*.+(ts|tsx)": [
      "yarn lint:staged",
      "yarn typecheck:staged",
      "yarn format"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "3.26.1",
